CLANG_CXX_LIBRARY = libstdc++
CONFIGURATION_BUILD_DIR = $PODS_CONFIGURATION_BUILD_DIR/ShapeKit
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/geos" "$PODS_CONFIGURATION_BUILD_DIR/proj4"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Public" ${PODS_ROOT}/geos/include ${PODS_ROOT}/geos/capi
OTHER_LDFLAGS = -ObjC -l"stdc++" -framework "CoreLocation"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/ShapeKit
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
